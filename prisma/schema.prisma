generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            Int      @id @default(autoincrement())
  username      String   @unique
  email         String   @unique
  provider      String?
  providerId    String?
  api_key       String?
  isApiVerified Boolean? @default(false)
}
